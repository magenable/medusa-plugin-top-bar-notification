{
  "name": "@magenable/medusa-plugin-top-bar-notification",
  "version": "1.0.5",
  "description": "This is a MedusaJS plugin that allows to display simple notices at the top of a storefront",
  "author": "Magenable",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/magenable/medusa-plugin-top-bar-notification"
  },
  "keywords": [
    "medusa-notifications",
    "medusa-warnings",
    "medusa-notices",
    "plugin",
    "medusa-plugin-other",
    "medusa-plugin",
    "medusa-v2"
  ],
  "engines": {
    "node": ">=20"
  },
  "files": [
    ".medusa/server"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./workflows": "./.medusa/server/src/workflows/index.js",
    "./.medusa/server/src/modules/*": "./.medusa/server/src/modules/*/index.js",
    "./modules/*": "./.medusa/server/src/modules/*/index.js",
    "./providers/*": "./.medusa/server/src/providers/*/index.js",
    "./*": "./.medusa/server/src/*.js",
    "./admin": {
      "import": "./.medusa/server/src/admin/index.mjs",
      "require": "./.medusa/server/src/admin/index.js",
      "default": "./.medusa/server/src/admin/index.js"
    }
  },
  "scripts": {
    "build": "medusa plugin:build",
    "dev": "medusa plugin:develop",
    "prepublishOnly": "medusa plugin:build"
  }
}
